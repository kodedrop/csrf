{
  "name": "kodedrop/csrf",
  "description": "A simple hello world composer package",
  "description": "Csrf In PHP",
  "keywords": [
    "php csrf",
    "kodedrop csrf",
    "kodedrop"
  ],
  "homepage": "https://kodedrop.com/package/csrf",
  "license": "MIT",
  "authors": [
    {
      "name": "kodedrop.com",
      "email": "info@kodedrop.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Kodedrop\\": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php -r \"chmod('./tmp', 0777);\""
    ],
    "cs": "@php vendor/bin/phpcs -v --report-width=160 --standard=ruleset.xml --severity=1 --warning-severity=0 --extensions=php src utils tests",
    "test": "@php vendor/bin/phpunit",
    "coverage": "@php vendor/bin/phpunit --coverage-text"
  },
  "require": {
    "php": "^5.6 || ^7.0 || ~8.0.0 || ~8.1.0 || ~8.2.0"
  }
}
